{
  "type": "kafka",
  "spec": {
    "dataSchema": {
      "dataSource": "wikimedia.recentchange",
      "timestampSpec": {
        "column": "timestamp",
        "format": "posix"
      },
      "dimensionsSpec": {
        "dimensions": [
          "id",
          "title",
          "user",
          "type",
          "namespace",
          "comment",
          "server_name",
          "bot",
          "minor",
          "wikidata_label",
          "wikidata_desc"
        ]
      },
      "metricsSpec": [
        {
          "name": "count",
          "type": "count"
        }
      ],
      "granularitySpec": {
        "type": "uniform",
        "segmentGranularity": "HOUR",
        "queryGranularity": "NONE",
        "rollup": false
      }
    },
    "ioConfig": {
      "topic": "wikimedia.recentchange",
      "inputFormat": {
        "type": "json"
      },
      "consumerProperties": {
        "bootstrap.servers": "kafka-kraft:29092"
      },
      "taskCount": 1,
      "replicas": 1,
      "useEarliestOffset": true
    }
  }
}
